  <package id="2" version="2.5.7" url="" name="ImageGen" folder="46988a59-3996-4e0a-9b1c-ec91c7fefdde" packagepath="" repositoryGuid="65194810-1f85-11dd-bd0b-0800200c9a66" packageGuid="cbc37b15-4a53-4c3f-b8a0-e49543d84192" hasUpdate="False" enableSkins="False" skinRepoGuid="00000000-0000-0000-0000-000000000000">
